        <class_source>import java.util.List;
import java.util.Date;

String createTableSql = "";
String excelColumnTemp = "";

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
  if (first) {
    first = false;

    /* TODO: Your code here. (Using info fields)

    FieldHelper infoField = get(Fields.Info, "info_field_name");

    RowSet infoStream = findInfoRowSet("info_stream_tag");

    Object[] infoRow = null;

    int infoRowCount = 0;

    // Read all rows from info step before calling getRow() method, which returns first row from any
    // input rowset. As rowMeta for info and input steps varies getRow() can lead to errors.
    while((infoRow = getRowFrom(infoStream)) != null){

      // do something with info data
      infoRowCount++;
    }
    */
	
	

  }

  Object[] r = getRow();

  if (r == null) {
    setOutputDone();
    return false;
  }
	
	

  // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
  // enough to handle any new fields you are creating in this step.
  r = createOutputRow(r, data.outputRowMeta.size());

  /* TODO: Your code here. (See Sample)

  // Get the value from an input field
  String foobar = get(Fields.In, "a_fieldname").getString(r);

  foobar += "bar";
    
  // Set a value in a new output field
  get(Fields.Out, "output_fieldname").setValue(r, foobar);

  */

	String year = get(Fields.In, "year").getString(r).trim();
	String tableNameSuffix = get(Fields.In, "year").getString(r).trim();

	String newTableSql = "";
	String table_name = get(Fields.In, "table_name").getString(r);
	
	if(createTableSql.equals("")){
		StringBuffer sb = new StringBuffer();
		StringBuffer excelColumn = new StringBuffer();
		List&lt;RowSet> list = getInputRowSets();
		RowSet row = (RowSet)list.get(0);
		RowMetaInterface rowMeta = row.getRowMeta();
		List&lt;ValueMetaInterface> valueMetaList = rowMeta.getValueMetaList();
		sb.append("CREATE TABLE IF NOT EXISTS " + table_name + " (");
   		for(ValueMetaInterface value : valueMetaList){
			if(fieldNameFilter(value.getName())){
				continue;
			}
			transform(sb,excelColumn,value.getName(),value.getTypeDesc(),value.getLength());
    		super.logBasic(value.getName() + "---" + value.getTypeDesc() + "-----" + value.getLength());
    	}
		String temp = sb.substring(0,sb.length() - 1);
		temp = temp + ");";
		createTableSql = temp;
		excelColumnTemp = excelColumn.substring(0,excelColumn.length() - 1);
	}
		newTableSql = createTableSql.replace("tableYearTemplate",year);
		get(Fields.Out, "table_sql").setValue(r, newTableSql);
		get(Fields.Out, "table_name").setValue(r, table_name.replace("tableYearTemplate",year));
		get(Fields.Out, "excel_column").setValue(r, excelColumnTemp);
	
  // Send the row on to the next step.
  putRow(data.outputRowMeta, r);
  return true;
}

private void transform(StringBuffer sb,StringBuffer excelColumn,String fieldName,String typeDesc,int length){
        String[] fieldArr = fieldName.split("-");
        String comment = fieldArr.length >= 2 ? fieldArr[1] : "";
        sb.append(" `"+ fieldName + "` ");
		excelColumn.append(fieldName + "$" + length + "#" + typeDesc + ",");
        if(length == -1){
            length = 10;
        }
		
			switch (typeDesc){
            case "String" :
                sb.append("varchar(" + length +") NULL COMMENT '" + comment + "'");
                break;
            case "Number" :
                sb.append("decimal(" + length+ ",2) NULL COMMENT '" + comment + "'");
                break;
            case "Integer":
                sb.append("int(" + length + ") NULL COMMENT '" + comment + "'");
                break;
            case "Date":
                sb.append("datetime NULL COMMENT '" + comment + "'");
                break;
        	}

	
		
        sb.append(" ,");
    }

private boolean fieldNameFilter(String fieldName){
        String[] filterArr = new String[]{"table_sql","excel_column","table_name"};
        for(String filterName : filterArr){
            if(filterName.equals(fieldName)){
                return true;
            }
        }
        return false;
}
